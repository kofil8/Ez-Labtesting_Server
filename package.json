{
  "name": "ez-lab-testing",
  "version": "1.0.0",
  "description": "Ez Lab Testing is an online laboratory test ordering and result delivery platform.",
  "main": "dist/server.js",
  "author": {
    "name": "Ez Lab Testing",
    "developer": "Engr. Mohammed Kofil",
    "email": "info@ezlabtesting.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --runInBand",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "s3:migrate": "ts-node -r tsconfig-paths/register src/scripts/migrate-to-s3.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@prisma/client": "6.8.2",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.7.7",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.0",
    "firebase-admin": "^13.6.0",
    "helmet": "^7.2.0",
    "http-status": "2.1.0",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "nodemailer": "7.0.4",
    "nodemailer-smtp-transport": "^2.7.4",
    "rate-limit-redis": "^4.3.0",
    "redis": "^5.10.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.0.2",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.3",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.8",
    "@types/multer": "^2.0.0",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/web-push": "^3.6.4",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "jest": "^30.0.0",
    "prisma": "6.8.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
